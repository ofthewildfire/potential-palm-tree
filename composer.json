{
    "name": "ofthewildfire/filament-resource-builder",
    "description": "A Filament plugin to quickly add new resources from the GUI inside Filament apps.",
    "keywords": [
        "filament",
        "laravel",
        "resource-builder",
        "dynamic-resources",
        "cms",
        "admin-panel"
    ],
    "homepage": "https://github.com/ofthewildfire/potential-palm-tree",
    "support": {
        "issues": "https://github.com/ofthewildfire/potential-palm-tree/issues",
        "source": "https://github.com/ofthewildfire/potential-palm-tree"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ofthewildfire",
            "email": "103148299+ofthewildfire@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Fuascailtdev\\FilamentResourceBuilder\\": "src/",
            "Fuascailtdev\\FilamentResourceBuilder\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fuascailtdev\\FilamentResourceBuilder\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fuascailtdev\\FilamentResourceBuilder\\FilamentResourceBuilderServiceProvider"
            ],
            "aliases": {
                "FilamentResourceBuilder": "Fuascailtdev\\FilamentResourceBuilder\\Facades\\FilamentResourceBuilder"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}